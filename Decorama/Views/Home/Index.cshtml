@using Decorama.Models
@model Decorama.Models.HomeViewModel
@{
    Layout = "~/Views/Shared/_LayoutTemplate.cshtml";


}

<!-- Blog Section -->
<section id="blog" class="visible-over">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 col-md-12">
                <h4 class="section-name fade-animate">Inicio<i class="fa fa-minus" aria-hidden="true"></i></h4>
            </div>
        </div>
        <div class="row pos-r">
            <div class="col-sm-12 col-md-6 blog-image-cell">
                @try
                {
                    <div class="blog-image fade-animate" style="background: url(../img/@SessionH.Imagenes.First(x=>x.Tipo == "Inicio").Ruta) no-repeat center center"></div>
                }
                catch
                {
                     <div class="blog-image fade-animate"></div> 
                }

            </div>
            <div class="col-sm-12 col-md-6">
                <div style="text-align:center">
                    <h1 class="section-heading fade-animate">@Model.SeccionInicio.Titulo</h1>
                    @*<h4 class="blog-title">may 2017</h4>*@
                    <p class="blog-text">
                        @Html.Raw(Model.SeccionInicio.Contenido)
                    </p>
                    @*<a href="#portfolioModal1" data-toggle="modal" class="page-scroll btn btn-xl fade-animate ">TRABAJOS  ;)</a>*@
                </div>
            </div>
        </div>
    </div>
</section>

<!-- About Section-->
<section id="about">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h4 class="section-name fade-animate">Nosotros<i class="fa fa-minus" aria-hidden="true"></i></h4>
            </div>
        </div>
        <div class="row pos-r">
            <div class="col-md-10 col-md-offset-1">
                <h1 class="about-heading section-heading fade-animate ">@Model.SeccionNosotros.Titulo</h1>
                <div class="about-text">
                    @Html.Raw(Model.SeccionNosotros.Contenido)
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Team Section -->
<section id="team" class="visible-over">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h4 class="section-name fade-animate">Equipo<i class="fa fa-minus" aria-hidden="true"></i></h4>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-7">
                <h1 class="team-heading section-heading fade-animate ">@Model.SeccionEquipo.Titulo</h1>
                @*<p class="team-text">
                        Somos un equipo joven con experiencia en el diseño y decoración de interiores. Con estudios profesionales, nos destacamos <b>por nuestra originalidad</b> al momento de plantear la solución a tu necesidad.
                    </p>
                    <p class="team-text">
                        <h3>Carolina Morales</h3>. Diseñadora de profesión, bla bla bla bla bla bla
                    </p>
                    <p class="team-text">
                        <h3>Susana Díaz</h3>. Diseñadora de profesión, bla bla bla bla bla bla
                    </p>*@
                @Html.Raw(Model.SeccionEquipo.Contenido)
            </div>
            <div class="col-sm-12 col-md-4 col-md-offset-1 team-image-cell">
                <div>
                    <div class="col-xs-12">
                        @try
                        {
                            <div class="team-cell fade-animate" style="background: url(../img/@SessionH.Imagenes.First(x=>x.Tipo == "Equipo" && x.Nombre.ToLower() == "carolina").Ruta) no-repeat center center"></div>
                        }
                        catch
                        {
                            <div class="team-cell fade-animate"></div>

                        }
                        <h5>Carolina Morales</h5>
                        <h6>Diseñadora</h6>
                    </div>
                    <div class="col-xs-12">
                        @try
                        {
                            <div class="team-cell fade-animate" style="background: url(../img/@SessionH.Imagenes.First(x=>x.Tipo == "Equipo" && x.Nombre.ToLower() == "susana").Ruta) no-repeat center center"></div>
                        }
                        catch
                        {
                            <div class="team-cell fade-animate"></div>
                        }
                        <h5>Susana Díaz</h5>
                        <h6>Diseñadora</h6>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!--Quote Section-->
<section id="quote">
    <div class="bg-filter"></div>
    <div class="container" data-type="content">
        <div class="row">
            <div class="col-md-12">
                <h5 class="fade-animate">Frase inspiradora</h5>
                <div class="quote-separator"></div>
                <blockquote>
                    "No hay estilos definidos, hay un estilo para cada cliente"
                    <footer class="quote-footer">Decorama</footer>
                </blockquote>
            </div>
        </div>
    </div>
</section>

<!-- Services Section -->
<section id="services" class="visible-over">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h4 class="section-name fade-animate">Servicios<i class="fa fa-minus" aria-hidden="true"></i></h4>
            </div>
        </div>
        <div class="row pos-r">
            <div class="service-image-container col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-0">
                @try
                {
                    <div class="service-image1" style="background: url(../img/@SessionH.Imagenes.Where(x=>x.Tipo == "Servicios").ToList()[0].Ruta) no-repeat center center !important"></div>

                }
                catch
                {
                    <div class="service-image1"></div>
                }
                @try
                {
                    <div class="service-image2 fade-animate" style="background: url(../img/@SessionH.Imagenes.Where(x=>x.Tipo == "Servicios").ToList()[1].Ruta) no-repeat center center !important"></div>

                }
                catch
                {
                    <div class="service-image2 fade-animate"></div>
                }
            </div>
            <div class="col-sm-12 col-md-6 right">
                <div>
                    <div class="col-sm-6 col-md-12 service-info">
                        <i class="fa fa-camera-retro fa-3x pull-left service-icon" aria-hidden="true"></i>
                        <div class="service-cell">
                            <h4 class="service-heading fade-animate">Fotografía y decoración</h4>
                            <div class="service-separator"></div>
                            <p class="service-text fade-animate">
                                Matrimonios, celebraciones o momentos importantes. Podemos decorar y hacer tu evento más
                                comodo para ti y tus invitados, capturando para la eternidad, esos momentos inolvidables.
                            </p>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-12 service-info">
                        <i class="fa fa-barcode fa-3x pull-left service-icon" aria-hidden="true"></i>
                        <div class="service-cell">
                            <h4 class="service-heading fade-animate">Diseños personalizados</h4>
                            <div class="service-separator"></div>
                            <p class="service-text fade-animate">
                                Contamos con productos exclusivos hechos por Decorama. ¡Visita nuestra tienda!
                            </p>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-12 service-info">
                        <i class="fa fa-gift fa-3x pull-left service-icon" aria-hidden="true"></i>
                        <div class="service-cell">
                            <h4 class="service-heading fade-animate">Ocasiones especiales</h4>
                            <div class="service-separator"></div>
                            <p class="service-text fade-animate">
                                Podemos asesorarte para un cumpleaños importante, graduación u otra celebración que necesite
                                un ambiente acorde a tu estilo.
                            </p>
                        </div>
                    </div>
                    <div class="col-sm-6 col-md-12 service-info">
                        <i class="fa fa-pagelines fa-3x pull-left service-icon" aria-hidden="true"></i>
                        <div class="service-cell">
                            <h4 class="service-heading fade-animate">Arreglos florales</h4>
                            <div class="service-separator"></div>
                            <p class="service-text fade-animate">
                                Trabajamos con florerías locales, así aseguramos flores frescas y de las mejores variedades.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

