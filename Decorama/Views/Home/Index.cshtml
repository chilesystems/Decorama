<!-- Contact Section -->
<section id="contact">
    <div class="bg-filter"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h4 class="section-name fade-animate">Contacto<i class="fa fa-minus" aria-hidden="true"></i></h4>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-10 col-sm-offset-1 col-md-6 col-md-offset-3">
                <h1 class="contact-heading section-heading fade-animate delay4">¡Escríbenos!</h1>


                @using (Html.BeginForm("Contacto", "Home", FormMethod.Post, new { @class = "form", @id = "form_enviar" }))
                {
                    <div class="row">
                        <div class="form-group col-md-12">
                            <input type="text" class="form-control" placeholder="Nombre *" name="Form.Nombre" id="name" required data-validation-required-message="Ingresa tu nombre">
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group col-md-12">
                            <input type="email" class="form-control" placeholder="Email *" name="Form.Email" id="email" required data-validation-required-message="Ingresa tu mail para que podamos contactarte.">
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="clearfix"></div>
                        <div class="form-group col-md-12">
                            <textarea class="form-control" placeholder="Mensaje *" name="Form.Mensaje" id="message" required data-validation-required-message="¿Que nos quieres decir?"></textarea>
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="col-md-12 text-center fade-animate">
                            <div id="success"></div>
                            <button type="submit" class="btn btn-xl">Enviar</button>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</section>

